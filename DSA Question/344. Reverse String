/**
 * @param {character[]} s
 * @return {void} Do not return anything, modify s in-place instead.
 */
var reverseString = function(s) {
    for ( i = 0 ; i < s.length/2; i++ ) {
        temp = s[i]
        s[i] = s[s.length - 1 - i]
        // this will make the current i be the num reflecting it in the back
        s[s.length - 1 - i] = temp
    };
};